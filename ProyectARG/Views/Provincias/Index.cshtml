@{
    ViewData["Title"] = "Provincias";
}



<button type="button" class="btn btn-primary" onclick="NuevaProvincia()">Crear provincia</button>


<table class="table" id="table">
  <thead>
    <tr>
      <th scope="col">Nombre</th>
      <th scope="col">Editar</th>
      <th scope="col">Eliminar</th>
    </tr>
  </thead>
  <tbody id="tbody">
  </tbody>
</table>


<div class="modal" tabindex="-1" id="ProvinciaModal">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <h5 id="tituloModal"></h5>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>
      <div class="modal-body">
        <input type="hidden" value="0" id="ProvinciaID">
        <label for="Nombre">Nombre</label>
        <input type="text" id="ProvinciaNombre" class="form-control">
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-bs-dismiss="modal" onclick="">Cancelar</button>
        <button type="button" class="btn btn-primary" onclick="GuardarProvincia()">Guardar Provincia</button>
      </div>
    </div>
  </div>
</div>


@section Scripts{
  <script src="~/js/provincias.js"></script>
}